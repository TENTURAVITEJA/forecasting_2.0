
{
  "name": "ai-forecasting-webapp",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "bootstrap": "npm --prefix server install && npm --prefix web install",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:web\"",
    "dev:server": "npm --prefix server run dev",
    "dev:web": "npm --prefix web run dev",
    "build": "npm --prefix server run build && npm --prefix web run build",
    "start": "npm --prefix server start"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
